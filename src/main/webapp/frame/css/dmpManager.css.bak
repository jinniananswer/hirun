.p_paper { max-width:60em; margin:0 auto;}
.s_pad .p_paper,
.s_pc .p_paper { padding:1.43em 1.43em 1.43em 1.43em;}


.s_pad .p_nav,
.s_pc .p_nav { display:block; position:absolute; left:50%; z-index:2; width:10em; margin-left:31em; top:1.43em;}
.p_nav .content { box-shadow:none;}
.p_intro > .l_colItem:first-child { width:8em; flex:none;}
.p_intro > .l_colItem:first-child img { width:8em; height:auto;}



.p_index { font-weight:200;}
.p_index > .data { height:2.67em; font-weight:200; font-family:"microsoft yahei"; line-height:2.67em; font-size:150%; color:#fff; background:#4d5b65; box-sizing:border-box;}
.p_index > .data .logo { display:inline-block; padding:0 0.67em; cursor:pointer;}
.p_index > .data .logo img { height:2.67em; box-sizing:border-box; padding:0.5em 0; margin-right:0.6em;}
.p_index > .data .logo:hover { color:#f3b100;}
.p_index > .side { position:absolute; left:0; top:4em; left:0; width:15em; bottom:0; z-index:2; box-sizing:border-box; border-right:1px solid #d4d4d4; background:#fff; overflow:auto; padding:0 0.5em;}
.p_index > .side:after { content:""; height:1em; display:block;}
// .p_index > .side .title { line-height:3; padding:0 0.3em; font-size:120%; font-weight:400; color:#68a2e5;}
.p_index > .side .content { line-height:2.4; padding:0.8em 0;}
.p_index > .side .content li { cursor:pointer; padding:0 0.8em;}
.p_index > .side .content li:hover { color:#f3b100; background:#f7f7f7;}
.p_index > .side .content li.on,
.p_index > .side .content li.on:hover { color:#fff; background:#8fc320;}
.p_index > .main { position:absolute; left:15em; top:4em; right:0; bottom:0;}
.p_index > .main iframe { border:0 none; height:100%; width:100%; vertical-align:top; display:block;}
.p_index > .nav { display:none;}
.p_index + .c_msg { top:4em; left:15em;}
.p_home { position:absolute; left:15em; top:0; right:0; bottom:0;}
.p_homeList > ul { overflow:hidden;}
.p_homeList > ul > li { float:left; width:19.5em; height:14em; margin-right:1.5em; margin-bottom:1.5em; background:#fff; border:1px solid #d4d4d4; cursor:pointer;}
.p_homeList > ul > li:hover { background:#f7f7f7;}



// .s_phone .p_index > .data { display:none;}
.s_phone .p_index > .side { width:100%; top:3.57em; bottom:3.57em; display:none;}
.s_phone .p_index > .side-show { display:block;}
.s_phone .p_index > .main { top:0; bottom:3.57em;}
.s_phone .p_index + .c_msg { left:0; bottom:3.57em; top:3.57em;}
.s_phone .p_index > .nav { display:block; position:absolute; bottom:0; width:100%; background:#fff; color:#4d5b65; height:3.57em; border-top:1px solid #d4d4d4; z-index:4;}
.s_phone .p_index > .nav > ul { display:flex;}
.s_phone .p_index > .nav > ul > li { flex:1; display:block; text-align:center; font-size:120%; line-height:2.98em; border-left:1px solid #d4d4d4; cursor:pointer;}
.s_phone .p_index > .nav > ul > li:hover { background:#f7f7f7;}
.s_phone .p_index > .nav > ul > li span { display:inline-block; vertical-align:middle; margin-right:1em;}
.s_phone .p_home { left:0;}
.s_phone .p_homeList > ul > li { float:none; width:auto; margin-right:0; margin-bottom:0.71em;}